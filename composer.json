{
    "name": "arrocy/laravel-version",
    "description": "Version management package for Laravel",
    "keywords": [
        "laravel",
        "version"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arrocy",
            "email": "info@arrocy.com",
            "homepage": "https://arrocy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": ">=5.2.0"
    },
    "autoload": {
        "psr-4": {
            "Arrocy\\Version\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arrocy\\Version\\Provider"
            ],
            "aliases": {
                "Version": "Arrocy\\Version\\Facade"
            }
        }
    }
}
